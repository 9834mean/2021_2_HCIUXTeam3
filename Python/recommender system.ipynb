    "# 두연님 함수 들어갈 부분\n",
    "\n",
    "def make_doc2vec_data(data, column, t_document=False):\n",
    "    data_doc = []\n",
    "    for tag, doc in zip(data.index, data[column]):\n",
    "        doc = doc.split(\" \")\n",
    "        data_doc.append(([tag], doc))\n",
    "    if t_document:\n",
    "        data = [TaggedDocument(words=text, tags=tag) for tag, text in data_doc]\n",
    "        return data\n",
    "    else:\n",
    "        return data_doc"
   ]
